{
  "name": "spaden",
  "version": "6.0.2",
  "description": "The FINN.no front-end CSS framework built on top of OOCSS",
  "scripts": {
    "format-source-code": "REALLY_DO_IT=1 node ./scripts/format-stylesheet.js",
    "maven:install": "npm run package && node maven install",
    "maven:snapshot": "npm run package && node maven snapshot",
    "maven:deploy": "npm run package && node maven deploy",
    "package": "gulp",
    "build-docs": "mkdir -p out && npm run build-docs:html && npm run build-docs:js && npm run build-docs:css",
    "build-docs:html": "node ./docs/generate-doc-html.js",
    "build-docs:js": "browserify docs/docs.js --outfile out/docs.js",
    "build-docs:css": "gulp compile-doc-sheet && gulp package-to-docs",
    "test": "npm run package",
    "push-package-publish": "git push --follow-tags && npm run package && npm publish --registry 'https://registry.npmjs.org' && npm run maven:deploy",
    "release:patch": "npm version patch && npm run push-package-publish",
    "release:minor": "npm version minor && npm run push-package-publish",
    "release:major": "npm version major && npm run push-package-publish",
    "start": "gulp",
    "watch": "gulp watch"
  },
  "devDependencies": {
    "browserify": "^13.1.0",
    "colors": "^1.1.0",
    "css-color-converter": "^1.0.2",
    "del": "^2.2.2",
    "eslint": "^3.4.0",
    "eslint-config-finn": "^1.0.0-alpha.11",
    "filewalker": "^0.1.2",
    "globby": "^6.0.0",
    "gulp": "^3.6.2",
    "gulp-bump": "^2.4.0",
    "gulp-clean-css": "^2.0.7",
    "gulp-concat": "^2.2.0",
    "gulp-filesize": "0.0.6",
    "gulp-filter": "^4.0.0",
    "gulp-git": "^1.2.4",
    "gulp-gzip": "^1.1.0",
    "gulp-inject-string": "^1.1.0",
    "gulp-load-plugins": "^1.2.4",
    "gulp-postcss": "^6.0.1",
    "gulp-rename": "^1.2.0",
    "gulp-replace": "^0.5.3",
    "gulp-sourcemaps": "^1.5.2",
    "gulp-tag-version": "^1.1.0",
    "gulp-tar": "^1.0.0",
    "gulp-util": "^3.0.0",
    "handlebars": "^4.0.5",
    "highlight.js": "^9.6.0",
    "marked": "^0.3.6",
    "maven-deploy": "^1.3.0",
    "pify": "^2.3.0",
    "spaden-postcss-config": "^1.1.0",
    "tar": "^2.1.1"
  },
  "contributors": [
    "Snorre Nygaard <skadegryd>",
    "Anders Johnsen <antidecaf>",
    "David Hasäther <hzr>",
    "Espen Dalløkken <leftieFriele>",
    "Sveinung Røsaker <sveisvei>",
    "Gregers Rygg <gregers@finn.no>",
    "Tor Arne Kvaløy <tor.arne.kvaloy@finn.no>"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/finn-no/spaden"
  },
  "dependencies": {}
}
